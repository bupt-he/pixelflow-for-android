<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="processing.SoftBodyActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:ignore="UselessLeaf">

        <Button
            android:id="@+id/btn_fluid_basic"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_margin="5dp"
            android:onClick="onClickButton"
            android:text="Fluid_Basic"
            tools:ignore="OnClick" />
        <Button
            android:id="@+id/btn_fluid_custom_particles"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_margin="5dp"
            android:onClick="onClickButton"
            android:text="Fluid_CustomParticles"
            tools:ignore="OnClick" />

        <Button
            android:id="@+id/btn_fluid_first_blood"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_margin="5dp"
            android:onClick="onClickButton"
            android:text="Fluid_FirstBlood"
            tools:ignore="OnClick" />

        <Button
            android:id="@+id/btn_fluid_get_start"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:layout_margin="5dp"
            android:onClick="onClickButton"
            android:text="Fluid_GetStarted"
            tools:ignore="OnClick" />




    </LinearLayout>
</androidx.core.widget.NestedScrollView>